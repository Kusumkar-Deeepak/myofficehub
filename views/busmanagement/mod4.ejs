    <title>PolyTransport</title>
    <style>
        /* Your custom styles go here */
        body {
            background: #eee;
            display: grid;
            grid-template-columns: repeat(3, 1fr); /* 3 columns */
            gap: 20px; /* Adjusted gap between cards */
            padding: 20px; /* Added padding for better spacing */
        }

        .card {
            border: none;
            overflow: hidden;
            border-radius: 8px;
            cursor: pointer;
            width: 100%; /* Adjusted width to fill the grid column */
            height: 450px; /* Adjusted height */
            position: relative; /* Added position relative */
        }

        .card:before,
        .card:after {
            content: "";
            position: absolute;
            left: 0;
            top: 0;
            width: 4px;
            height: 100%;
            transition: all 0.5s;
            transform-origin: bottom;
        }

        .card:before {
            background-color: #E1BEE7;
            transform: scaleY(1);
        }

        .card:after {
            background-color: #8E24AA;
            transform: scaleY(0);
        }

        .card:hover::after {
            transform: scaleY(1);
        }

        .text-center {
            text-align: center;
            padding: 10px; /* Added padding for card content */
        }

        .buttons {
            display: flex;
            flex-direction: column;
            align-items: center;
            margin-top: 10px;
        }

        .buttons button {
            border: 1px solid #8E24AA !important;
            background-color: #8E24AA;
            color: #fff;
            height: 40px;
            width: 100%; /* Make the button full width */
            margin-top: 10px; /* Added margin between button and content */
        }

        .buttons button:hover {
            background-color: #6a1b9a;
        }
        .card img {
    max-width: 100%;
    height: 7.5rem;
    width: 7.5rem;
    display: block;
    margin: auto;
    transition: transform 0.3s ease-in-out, box-shadow 0.3s ease; 
    border: 0.125rem solid transparent;
    border-radius: 0.5rem; 
    box-shadow: 0 0.25rem 0.5rem rgba(0, 0, 0, 0.1); 
}

.card:hover img {
    transform: scale(1.1);
    box-shadow: 0 0.5rem 1rem rgba(0, 0, 0, 0.2);
    /* border-color: #3498db; */
}
@media (max-width: 576px) {
        .card {
            flex-basis: calc(100% - 30px);
        }
    }

    @media (min-width: 577px) and (max-width: 767px) {
        .card {
            flex-basis: calc(50% - 30px);
        }
    }

    @media (min-width: 768px) and (max-width: 991px) {
        .card {
            flex-basis: calc(33.33% - 30px);
        }
    }

    @media (min-width: 992px) {
        .card {
            flex-basis: calc(25% - 30px);
        }
    }    </style>
